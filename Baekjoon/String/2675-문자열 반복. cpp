#include <iostream>
#include <string>
using namespace std;

int t, r;
string word;

void concate() {
	cin >> r >> word;
	for (int i = 0; i < word.length(); i++) {
		string temp;
		for (int k = 0; k < r; k++) {
			temp += word[i];
		}
		cout << temp;
	}
	cout << endl;
}

int main() {
	cin >> t;
	for (int i = 0; i < t; i++) {
		concate();
	}
	return 0;
}
